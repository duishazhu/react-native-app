    "watch:wechat": "octopus build -w --progress --compress",
    "build:wechat": "octopus build --progress --compress",
    "watch:alipay": "node scripts/merge-mp-comp.js && octopus build --target alipay -w --progress --compress",
    "start": "DOTENV_CONFIG_PATH=.env DOTENV_CONFIG_DEFAULT_PATH=.env.default node -r @terminus/dotenv-defaults/config node_modules/react-native/local-cli/cli.js start --reset-cache",
    "dev": "node --max_old_space_size=4096 ./scripts/index.js --mode=start",
    "jarvis": "sh ./scripts/sh-lib/watch-dev.sh jarvis",
    "ios": "react-native run-ios",
    "pod": "sh ./scripts/pod.sh",
    "android": "cd android && sh ./gradlew clean installDebug -DENV_SWITCH=1",
    "build": "node --max_old_space_size=4096 ./scripts/index.js --mode=build",
    "server": "herd dicefile-docker.js",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "fresh-channel-manager": "node -r dotenv/config ./generate-channel-config.js",
    "postinstall": "patch-package --error-on-fail && jetifier",
    "icon:get": "sh ./scripts/sh-lib/get-icon.sh",
    "fmt": "prettier --write .",
    "upload-image": "node ./scripts/image-upload.js",
    "update-icon": "npx termix icon //at.alicdn.com/t/c/font_4332774_kk979egnb1.js --output=./src/fonts",
    "lint": "./node_modules/.bin/eslint src*/** --cache --ext es6 --ext jsx --ext js --ignore-pattern .gitignore --no-error-on-unmatched-pattern",
